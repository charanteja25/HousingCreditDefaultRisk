    {
      "name": "deb410df",
      "displayName": "Feature lift chart",
      "queryLines": [
        "WITH base AS (\n",
        "  SELECT\n",
        "    CASE\n",
        "      WHEN inst_payment_late_rate = 0 THEN '0%'\n",
        "      WHEN inst_payment_late_rate < 0.1 THEN '0–10%'\n",
        "      WHEN inst_payment_late_rate < 0.3 THEN '10–30%'\n",
        "      ELSE '>30%'\n",
        "    END AS bucket,\n",
        "    TARGET,\n",
        "    CASE\n",
        "      WHEN inst_payment_late_rate = 0 THEN 1\n",
        "      WHEN inst_payment_late_rate < 0.1 THEN 2\n",
        "      WHEN inst_payment_late_rate < 0.3 THEN 3\n",
        "      ELSE 4\n",
        "    END AS sort_key\n",
        "  FROM workspace.gold_creditrisk.train_dataset_b\n",
        "),\n",
        "overall AS (\n",
        "  SELECT AVG(TARGET) AS overall_default_rate FROM base\n",
        ")\n",
        "SELECT\n",
        "  bucket,\n",
        "  ROUND(AVG(TARGET), 4) AS bucket_default_rate,\n",
        "  ROUND(AVG(TARGET) / o.overall_default_rate, 2) AS lift,\n",
        "  b.sort_key\n",
        "FROM base b\n",
        "CROSS JOIN overall o\n",
        "GROUP BY bucket, b.sort_key, o.overall_default_rate\n",
        "ORDER BY b.sort_key;"
      ]
    },
    {
      "name": "3eeacde2",
      "displayName": "Feature coverage vs risk",
      "queryLines": [
        "SELECT\n",
        "  CASE\n",
        "    WHEN bureau_loan_cnt IS NULL THEN 'No Bureau'\n",
        "    ELSE 'Has Bureau'\n",
        "  END AS bureau_coverage,\n",
        "  COUNT(*) AS customers,\n",
        "  ROUND(AVG(TARGET), 4) AS default_rate\n",
        "FROM workspace.gold_creditrisk.train_dataset\n",
        "GROUP BY bureau_coverage;\n"
      ]
    },
    {
      "name": "bdaa9fa1",
      "displayName": "Interaction graph",
      "queryLines": [
        "SELECT\n",
        "  severe_dpd_flag,\n",
        "  frequent_late_payer_flag,\n",
        "  COUNT(*) AS customers,\n",
        "  ROUND(AVG(TARGET), 4) AS default_rate\n",
        "FROM workspace.gold_creditrisk.train_dataset_b\n",
        "GROUP BY severe_dpd_flag, frequent_late_payer_flag\n",
        "ORDER BY severe_dpd_flag, frequent_late_payer_flag;\n"
      ]
    }
