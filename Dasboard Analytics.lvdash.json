    {
      "name": "43fe3f8e",
      "displayName": "Cell 1",
      "queryLines": [
        "SELECT\n",
        "  CASE\n",
        "    WHEN inst_payment_late_rate = 0 THEN '0%'\n",
        "    WHEN inst_payment_late_rate < 0.1 THEN '0–10%'\n",
        "    WHEN inst_payment_late_rate < 0.3 THEN '10–30%'\n",
        "    ELSE '>30%'\n",
        "  END AS late_payment_bucket,\n",
        "  ROUND(AVG(TARGET), 4) AS default_rate,\n",
        "  COUNT(*) AS customers,\n",
        "  CASE\n",
        "    WHEN inst_payment_late_rate = 0 THEN 1\n",
        "    WHEN inst_payment_late_rate < 0.1 THEN 2\n",
        "    WHEN inst_payment_late_rate < 0.3 THEN 3\n",
        "    ELSE 4\n",
        "  END AS sort_key\n",
        "FROM workspace.gold_creditrisk.train_dataset_b\n",
        "GROUP BY late_payment_bucket, sort_key\n",
        "ORDER BY sort_key;"
      ]
    },
