        "--   COUNT(*) AS customers,\n",
        "--   ROUND(100.0 * COUNT(*) / SUM(COUNT(*)) OVER (), 2) AS pct\n",
        "-- FROM workspace.gold_creditrisk.train_dataset_b\n",
        "-- GROUP BY severe_dpd_flag\n",
